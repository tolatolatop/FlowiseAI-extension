{
    "manifest_version": 3,
    "name": "chatbot scripts",
    "description": "chatbot",
    "version": "0.1",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "js": [
                "content-script.js"
            ],
            "matches": [
                "https://example.com/"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "matches": [
                "<all_urls>"
            ],
            "resources": [
                "main.js"
            ]
        }
    ]
}